<Window x:Class="ResourceUsageMonitor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ResourceUsageMonitor"
        xmlns:oxy="http://oxyplot.org/wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
	<Grid>
		<oxy:PlotView x:Name="PlotView" Margin="0,50,0,0"/>
		<Label x:Name="Label_CpuStatus" Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top"/>
	</Grid>
</Window>
